{
    "manifest_version": 2,
    "name": "chromeExtensionTest",
    "short_name": "CXT",
    "version": "1.0",
    "description": "動作確認のためのテストアプリです",
    "icons": {
        "512": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Chromeの拡張テスト",
        "default_popup": "popup.html"
    },
    "permissions": [
        "background"
    ]
}